{
  "name": "beslutteroversikt",
  "version": "0.1.0",
  "private": true,
  "homepage": "/beslutteroversikt",
  "scripts": {
    "start": "cross-env REACT_APP_DEPLOYMENT_ENV=local craco start",
    "build:dev": "cross-env REACT_APP_DEPLOYMENT_ENV=development PUBLIC_URL=/ craco build --config craco.config.js",
    "build:prod": "cross-env REACT_APP_DEPLOYMENT_ENV=production PUBLIC_URL=/ craco build --config craco.config.js",
    "build:mock": "cross-env REACT_APP_DEPLOYMENT_ENV=local craco build",
    "test": "echo \"Tests not yet implemented\"",
    "prettier": "prettier --write \"src/**/*.{ts,tsx}\"",
    "prepare": "husky install",
    "pre-commit": "lint-staged --verbose"
  },
  "lint-staged": {
    "src/**/*.{json,less,css,md}": [
      "prettier --write"
    ],
    "src/**/*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "eslint --config .eslintrc.lintstaged.json"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "dependencies": {
    "@craco/craco": "7.1.0",
    "@navikt/ds-css": "^5.11.4",
    "@navikt/ds-icons": "^1.5.10",
    "@navikt/ds-react": "^5.3.1",
    "@navikt/fnrvalidator": "^1.3.0",
    "@navikt/navspa": "^5.0.1",
    "classnames": "^2.3.2",
    "constate": "^3.3.2",
    "craco-less": "3.0.1",
    "dayjs": "1.11.9",
    "lodash.debounce": "^4.0.8",
    "lodash.throttle": "^4.1.1",
    "nav-frontend-alertstriper": "^4.0.2",
    "nav-frontend-alertstriper-style": "^3.0.2",
    "nav-frontend-chevron": "^1.0.30",
    "nav-frontend-chevron-style": "^1.0.4",
    "nav-frontend-core": "^6.0.1",
    "nav-frontend-etiketter": "^3.0.3",
    "nav-frontend-etiketter-style": "^2.0.3",
    "nav-frontend-ikoner-assets": "^3.0.1",
    "nav-frontend-js-utils": "^1.0.20",
    "nav-frontend-lenker": "^2.0.2",
    "nav-frontend-lenker-style": "^2.0.2",
    "nav-frontend-paneler-style": "^2.0.2",
    "nav-frontend-skjema": "^4.0.6",
    "nav-frontend-skjema-style": "^3.0.3",
    "nav-frontend-spinner": "^3.0.1",
    "nav-frontend-spinner-style": "^1.0.2",
    "nav-frontend-typografi": "^4.0.2",
    "nav-frontend-typografi-style": "^2.0.2",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "^5.0.1",
    "react-select": "5.7.3",
    "typescript": "^4.9.5"
  },
  "devDependencies": {
    "@faker-js/faker": "7.6.0",
    "@types/lodash.debounce": "^4.0.7",
    "@types/react": "17.0.62",
    "@types/react-dom": "17.0.20",
    "@types/react-select": "^5.0.1",
    "cross-env": "^7.0.3",
    "file-loader": "^6.2.0",
    "husky": "8.0.3",
    "lint-staged": "13.2.3",
    "msw": "^2.0.10",
    "prettier": "3.0.0"
  },
  "msw": {
    "workerDirectory": "public"
  }
}
