  "private": true,
  "homepage": "/beslutteroversikt",
  "scripts": {
    "start": "cross-env REACT_APP_DEV=true craco start",
    "build": "cross-env REACT_APP_DEV=false PUBLIC_URL=/ craco build --config craco.config.js",
    "build:mock": "cross-env REACT_APP_DEV=true craco build",
    "test": "echo \"Tests not yet implemented\"",
    "prettier": "prettier --write \"src/**/*.{ts,tsx}\"",
    "prepare": "husky install",
    "pre-commit": "lint-staged --verbose"
  },
  "lint-staged": {
    "src/**/*.{json,less,css,md}": [
      "prettier --write"
    ],
    "src/**/*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "eslint --config .eslintrc.lintstaged.json"
    ]
  },
