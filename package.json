{
  "name": "beslutteroversikt",
  "version": "0.1.0",
  "type": "module",
  "private": true,
  "homepage": "/beslutteroversikt",
  "scripts": {
    "start": "vite --mode lokal",
    "build:mock": "tsc && vite build --mode lokal --base=/beslutteroversikt/",
    "build:dev": "tsc && vite build --mode development",
    "build:prod": "tsc && vite build --mode production",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "prettier": "prettier --write \"src/**/*.{ts,tsx}\"",
    "prepare": "husky install",
    "pre-commit": "lint-staged --verbose",
    "test": "echo \"Tests not yet implemented\""
  },
  "lint-staged": {
    "src/**/*.{json,less,css,md}": [
      "prettier --write"
    ],
    "src/**/*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "eslint --config eslint.config.lintstaged.js"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "dependencies": {
    "@navikt/aksel-icons": "5.12.0",
    "@navikt/ds-css": "5.12.0",
    "@navikt/ds-react": "5.15.1",
    "@navikt/fnrvalidator": "1.3.0",
    "@navikt/navspa": "6.0.1",
    "classnames": "2.3.2",
    "constate": "3.3.2",
    "dayjs": "1.11.10",
    "lodash.debounce": "4.0.8",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "typescript": "5.2.2"
  },
  "devDependencies": {
    "@faker-js/faker": "8.3.1",
    "@types/lodash.debounce": "4.0.9",
    "@types/react": "18.2.45",
    "@types/react-dom": "18.2.18",
    "@typescript-eslint/eslint-plugin": "6.14.0",
    "@typescript-eslint/parser": "6.14.0",
    "@vitejs/plugin-react": "4.2.1",
    "eslint": "8.56.0",
    "husky": "8.0.3",
    "less": "4.2.0",
    "lint-staged": "15.2.0",
    "msw": "2.0.11",
    "prettier": "3.1.1",
    "vite": "5.0.12",
    "vitest": "1.0.4"
  },
  "msw": {
    "workerDirectory": "public"
  }
}
