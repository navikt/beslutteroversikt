{
  "name": "beslutteroversikt",
  "version": "0.1.0",
  "type": "module",
  "private": true,
  "homepage": "/beslutteroversikt",
  "scripts": {
    "start": "vite --mode lokal",
    "build:mock": "tsc && vite build --mode lokal --base=/beslutteroversikt/",
    "build:dev": "tsc && vite build --mode development",
    "build:prod": "tsc && vite build --mode production",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "prettier": "prettier --write \"src/**/*.{ts,tsx}\"",
    "prepare": "husky",
    "pre-commit": "lint-staged --verbose echo \"Linting files...\"",
    "test": "echo \"Tests not yet implemented\""
  },
  "lint-staged": {
    "src/**/*.{json,less,css,md}": [
      "prettier --write"
    ],
    "src/**/*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "eslint --config eslint.config.lintstaged.js"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "dependencies": {
    "@navikt/aksel-icons": "5.18.1",
    "@navikt/ds-css": "5.18.1",
    "@navikt/ds-react": "5.18.1",
    "@navikt/fnrvalidator": "1.3.0",
    "@navikt/navspa": "6.0.1",
    "classnames": "2.5.1",
    "constate": "3.3.2",
    "dayjs": "1.11.10",
    "lodash.debounce": "4.0.8",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "typescript": "5.3.3"
  },
  "devDependencies": {
    "@faker-js/faker": "8.4.1",
    "@types/lodash.debounce": "4.0.9",
    "@types/react": "18.2.55",
    "@types/react-dom": "18.2.19",
    "@typescript-eslint/eslint-plugin": "6.21.0",
    "@typescript-eslint/parser": "6.21.0",
    "@vitejs/plugin-react": "4.2.1",
    "eslint": "8.56.0",
    "husky": "9.0.11",
    "less": "4.2.0",
    "lint-staged": "15.2.2",
    "msw": "2.1.7",
    "prettier": "3.2.5",
    "vite": "5.1.1",
    "vitest": "1.2.2"
  },
  "msw": {
    "workerDirectory": "public"
  }
}
