{
  "name": "beslutteroversikt",
  "version": "0.1.0",
  "type": "module",
  "private": true,
  "homepage": "/beslutteroversikt",
  "scripts": {
    "start": "vite --mode lokal",
    "build:mock": "tsc && vite build --mode lokal --base=/beslutteroversikt/",
    "build:dev": "tsc && vite build --mode development",
    "build:prod": "tsc && vite build --mode production",
    "lint": "eslint src --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "prettier": "prettier --write \"src/**/*.{ts,tsx}\"",
    "prepare": "husky",
    "pre-commit": "lint-staged --verbose echo \"Linting files...\"",
    "test": "echo \"Tests not yet implemented\""
  },
  "lint-staged": {
    "src/**/*.{json,less,css,md}": [
      "prettier --write"
    ],
    "src/**/*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "eslint --config eslint.config.lintstaged.js"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "dependencies": {
    "@navikt/aksel-icons": "7.30.1",
    "@navikt/ds-css": "7.30.1",
    "@navikt/ds-react": "7.30.1",
    "@navikt/fnrvalidator": "2.1.5",
    "@navikt/navspa": "7.1.0",
    "classnames": "2.5.1",
    "constate": "3.3.3",
    "dayjs": "1.11.18",
    "lodash.debounce": "4.0.8",
    "react": "19.1.0",
    "react-dom": "19.1.0"
  },
  "devDependencies": {
    "@eslint/js": "9.35.0",
    "@faker-js/faker": "10.0.0",
    "@types/lodash.debounce": "4.0.9",
    "@types/react": "19.1.13",
    "@types/react-dom": "19.1.9",
    "@typescript-eslint/eslint-plugin": "8.43.0",
    "@typescript-eslint/parser": "8.46.0",
    "@vitejs/plugin-react": "5.0.2",
    "eslint": "9.35.0",
    "eslint-plugin-jsx-a11y": "6.10.2",
    "eslint-plugin-react": "7.37.5",
    "eslint-plugin-react-hooks": "5.2.0",
    "eslint-plugin-react-refresh": "0.4.20",
    "globals": "16.4.0",
    "husky": "9.1.7",
    "less": "4.4.1",
    "lint-staged": "16.1.6",
    "msw": "2.11.2",
    "prettier": "3.6.2",
    "typescript": "5.9.2",
    "typescript-eslint": "8.43.0",
    "vite": "7.1.5",
    "vitest": "3.2.4"
  },
  "msw": {
    "workerDirectory": "public"
  }
}
